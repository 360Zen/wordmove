local:
  vhost: "http://vhost.local"
  wordpress_path: "~/dev/sites/your_site"
  database:
    name: "database_name"
    user: "user"
    password: "password"
    host: "host"
remote:
  vhost: "http://remote.com"
  wordpress_path: "/var/www/your_site"
  database:
    name: "database_name"
    user: "user"
    password: "password"
    host: "host"
  ssh:
    user: "user"
    password: "password"
    host: "host"
    port: 30000


